#include<iostream>
using namespace std;

int main() {

	for (int i = 0; i < 6; i++)
		cout << i << " ";
	cout << "" << endl;
	for (int i = 0; i < 6; i++)
		cout << i << " ";


	cout << "" << endl;



	for (int i = 0; i < 2; i++) {
		for (int j = 0; j < 3; j++) {
			cout << "*";
		}
		cout << endl;
	}


	for (int i = 0; i < 1; i++) {
		for (int j = 0; j < 1; j++) {
			cout << "*";
		}
		cout << endl;
	}


	for (int i = 0; i < 1; i++) {
		for (int j = 0; j < 2; j++) {
			cout << "*";
		}
		cout << endl;
	}


	for (int i = 0; i < 1; i++) {
		for (int j = 0; j < 3; j++) {
			cout << "*";
		}
		cout << endl;
	}


	for (int i = 0; i < 1; i++) {
		for (int j = 0; j < 4; j++) {
			cout << "*";
		}
		cout << endl;
	}





}//end of main
